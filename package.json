{
  "name": "react-component-starter",
  "version": "0.0.2",
  "description": "Starter project for making reusable React Components. Includes linting, testing and example project setup.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "node config/server.js",
    "dev": "babel src/index.jsx -o lib/index.js --watch",
    "build": "rimraf lib && babel src/index.jsx -o lib/index.js",
    "build-docs": "rimraf docs && NODE_ENV=production webpack --config config/webpack.prod.js",
    "test": "mocha config/test.setup.js src/**/*.test.jsx"
  },
  "keywords": [
    "react"
  ],
  "author": "Altay Aydemir <altay.aydemir@gmail.com>",
  "license": "MIT",
  "homepage": "http://altayaydemir.github.io/react-component-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/altayaydemir/react-component-starter.git"
  },
  "bugs": {
    "url": "https://github.com/altayaydemir/react-component-starter/issues"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
